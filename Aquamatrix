local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
    if humanoid.RigType == Enum.HumanoidRigType.R15 then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JOHNGAMING23/MainScript/refs/heads/main/Aquamatrix%20R15"))();
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/JOHNGAMING23/MainScript/refs/heads/main/Aquamatrix%20R6"))();
end
